{
  "name": "sanmix",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "build:all": "nx run api:build:production && nx run client:build:production && npm run replace-placeholders-in-build",
    "build:dev": "nx run api:build && nx run ui:build:local",
    "api:start": "nx run api:serve --watch",
    "fe:start": "nx run ui:serve --configuration=local --verbose",
    "fe:build": "ng build",
    "fe:watch": "ng build --watch --configuration development",
    "fe:test": "ng test",
    "be:build": "nest build",
    "api:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "api:start:dev": "nest start --watch",
    "api:start:debug": "nest start --debug --watch",
    "api:start:prod": "node dist/main",
    "api:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "api:test": "jest",
    "api:test:watch": "jest --watch",
    "api:test:cov": "jest --coverage",
    "api:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "api:test:e2e": "jest --config ./test/jest-e2e.json",
    "database:format-schema": "prisma format",
    "database:generate-typings": "prisma generate",
    "database:gui": "prisma studio",
    "database:gui:prod": "npx dotenv-cli -e .env.prod -- prisma studio",
    "database:migrate": "prisma migrate deploy",
    "database:push": "prisma db push",
    "database:seed": "prisma db seed",
    "database:setup": "yarn database:push && yarn database:seed",
    "database:validate": "prisma validate",
    "replace-placeholders-in-build": "node ./replace.build.js"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^9.4.3",
    "@ng-icons/core": "24.0.0",
    "@ng-icons/ionicons": "24.0.0",
    "@nrwl/angular": "^16.6.0",
    "@nrwl/cli": "^15.9.3",
    "@prisma/client": "^4.12.0",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^6.3.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular/cli": "~15.0.3",
    "@angular/compiler-cli": "^15.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/eslint-plugin-nx": "15.0.0",
    "@nrwl/jest": "15.0.0",
    "@nrwl/nest": "15.0.0",
    "@nrwl/node": "15.0.0",
    "@nrwl/nx-cloud": "15.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~4.5.0",
    "jest": "29.3.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nx": "^15.9.2",
    "prettier": "^2.3.2",
    "prisma": "^4.12.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "~4.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
